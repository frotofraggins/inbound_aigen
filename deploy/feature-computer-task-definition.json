      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ops-pipeline/feature-computer-1m",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "features",
          "awslogs-create-group": "true"
        }

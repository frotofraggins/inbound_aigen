{
  "family": "ops-pipeline-signal-engine-1m",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::160027201036:role/ops-pipeline-ecs-task-role",
  "taskRoleArn": "arn:aws:iam::160027201036:role/ops-pipeline-ecs-task-role",
  "containerDefinitions": [
    {
      "name": "signal-engine",
            "image": "160027201036.dkr.ecr.us-west-2.amazonaws.com/ops-pipeline/signal-engine-1m@sha256:093bd5ab64d4f8cd5c906dca5812368c2d6b974b8fed585c04df9318f414f45a",
      "essential": true,
      "environment": [
        {"name": "AWS_REGION", "value": "us-west-2"}
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ops-pipeline/signal-engine-1m",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "signals",
          "awslogs-create-group": "true"
        }
      }
    }
  ]
}

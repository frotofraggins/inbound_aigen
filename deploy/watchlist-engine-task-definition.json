{
  "family": "ops-pipeline-watchlist-engine-5m",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::160027201036:role/ops-pipeline-ecs-task-role",
  "taskRoleArn": "arn:aws:iam::160027201036:role/ops-pipeline-ecs-task-role",
  "containerDefinitions": [
    {
      "name": "watchlist-engine",
      "image": "160027201036.dkr.ecr.us-west-2.amazonaws.com/ops-pipeline/watchlist-engine-5m:latest",
      "essential": true,
      "environment": [
        {"name": "AWS_REGION", "value": "us-west-2"}
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ops-pipeline/watchlist-engine-5m",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "watchlist",
          "awslogs-create-group": "true"
        }
      }
    }
  ]
}
